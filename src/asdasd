autorizados = ['cnievas', 'pmartinez', 'jluna']

if (operador in autorizados):
    actualizar_notif(subasta)
    bpm.service.notifyGroup(group='MENU' ,title="Menú cargado",description="Ya está disponible el menú del día , sound=True)
    bpm.reply(
